<div class="item active">
    <!-- form -->
    <form class="form-data" method="post" id="form-data">
        <div class="clearfix">
            <label>First Name <span class="required">*</span></label>
            <div><input type="text" name="name" class="field-keyboard-tool" data-required='true' /></div>
        </div>

        <div class="clearfix">
            <label>Last Name</label>
            <div><input type="text" name="desc" class="field-keyboard-tool" data-required='false' /></div>
        </div>
