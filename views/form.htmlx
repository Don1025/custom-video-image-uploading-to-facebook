        <div class="clearfix">
            <label>Gender <span class="required">*</span></label>
            <div data-required='true'>
                <input type="radio" name="sex" value="male" /> Male
                <input type="radio" name="sex" value="female" /> Female
            </div>
        </div>

        <div class="clearfix">
            <label>Company <span class="required">*</span></label>
            <div data-required='true' data-at-least='2' data-at-most='4'>
                <input type="checkbox" name="company" value="company - 1" /> Company - 1
                <input type="checkbox" name="company" value="company - 2" /> Company - 2
                <input type="checkbox" name="company" value="company - 3" /> Company - 3
                <input type="checkbox" name="company" value="company - 4" /> Company - 4
